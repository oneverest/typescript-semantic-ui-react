{
  "name": "ryanblog-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@tinymce/tinymce-react": "^3.3.2",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.5",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^4.3.5",
    "babel-plugin-relay": "^6.0.0",
    "graphql": "^14.5.7",
    "graphql.macro": "^1.4.2",
    "husky": "^3.0.5",
    "ifanrx-react-ueditor": "^2.3.0",
    "lint-staged": "^9.2.5",
    "prettier": "^1.18.2",
    "react": "^16.9.0",
    "react-ace": "^7.0.4",
    "react-dom": "^16.9.0",
    "react-quill": "^1.3.3",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.2",
    "react-styleguidist": "^9.1.16",
    "redux": "^4.0.4",
    "redux-devtools": "^3.5.0",
    "semantic-ui-react": "^0.88.1",
    "source-map-explorer": "^2.0.1",
    "typescript": "^3.6.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "craco eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^5.5.0",
    "@semantic-ui-react/craco-less": "^1.1.0",
    "semantic-ui-less": "^2.4.1"
  }
}
